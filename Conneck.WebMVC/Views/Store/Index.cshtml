@model IEnumerable<Conneck.Models.StoreList>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StoreID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StoreID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { storeID = item.StoreID}) |
            @Html.ActionLink("Details", "Detail", new { storeID = item.StoreID }) |
            @Html.ActionLink("Delete", "Delete", new { storeID = item.StoreID })
        </td>
    </tr>
}

</table>
